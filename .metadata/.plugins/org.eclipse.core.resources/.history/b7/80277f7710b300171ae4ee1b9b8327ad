package it.dstech.model;

import java.util.List;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;

@Entity
public class Album {
	@Id
	@GeneratedValue
	private int id;
	
	private String titolo;
	
	private List<Brano> listaBrani;
	
	private int durata;
}
